  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "antd": "^4.21.5",
    "apexcharts": "^3.27.1",
    "axios": "^0.27.2",
    "cssnano": "^5.1.12",
    "flowbite": "^1.4.7",
    "i18next": "^21.8.13",
    "install": "^0.13.0",
    "moment": "^2.29.4",
    "nprogress": "^0.2.0",
    "react": "^18.2.0",
    "react-apexcharts": "^1.3.9",
    "react-dom": "^18.2.0",
